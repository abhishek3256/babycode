{
  "version": 2,
  "buildCommand": "cd ielts-institute && npm run build",
  "outputDirectory": "ielts-institute/build",
  "installCommand": "cd ielts-institute && npm install --legacy-peer-deps",
  "framework": "create-react-app",
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "s-maxage=31536000,immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
